@model IList<Momntz.Data.Projections.Momentos.HomepageMomento>
@{
    ViewBag.Title = "title";
    Layout = "../Shared/_Layout.cshtml";
}
@foreach (var item in Model)
{
    <div class="all box black-and-white photography col2">

        <div class="post-53 post type-post status-publish format-standard hentry category-black-and-white category-photography tag-bw tag-fashion box-content has-content">

            <div class="img-container">
                <a href="#" rel="lightbox" >
                    <img  src="@item.Media.Url" class="feat-img wp-post-image" alt="tumblr_lp1myzLHBC1qlh91go1_1280" title="tumblr_lp1myzLHBC1qlh91go1_1280" /></a>
                <div class="actions">

                    <a href="@item.Media.Url" rel="gallery" title="A post with the minimal post template" class="view colorbox">Enlarge</a>

                    <a class="share">Share</a><a href="/#comments" class="comment"><span>0</span> Comment</a>

                    <div class="share-container">
                        <div class="share-icons">
                            <a href="#" class="share-window">
                                <img src="/content/images/twitter-ic-16.png" alt="Share on Twitter" /></a>

                            <a href="#" class="share-window">
                                <img src="/Content/images/facebook-ic-16.png" alt="Share on Facebook" />
                            </a>

                            <a href="#" title="Share on Tumblr">
                                <img src="/content/images/tumblr-ic-16.png" alt="Share on Tumblr" />
                            </a>

                        </div>
                        <!-- #share-icons -->
                    </div>
                    <!-- #share-container -->

                </div>
                <!-- #actions -->
            </div>
            <!-- #img-container -->
            
            <div class="post-content">
                <h2><a href="#" title="A post with the minimal post template" rel="bookmark">@item.Momento.Title</a></h2>
                <p>@item.Momento.Story</p>
            </div>
            <!-- #entry -->
            <div class="post-footer">
                <span class="category-ic"><a href="#" title="View all posts in Black and White" rel="category tag">Black and White</a>, <a href="#" title="View all posts in Photography" rel="category tag">Photography</a></span>
                <a href="#" class="shortlink tooltip" title="Shortlink">Shortlink</a>
            </div>

        </div>
        <!-- #box-content -->
    </div>
    <!-- #box -->
}